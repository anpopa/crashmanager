###############################################################################
#
#  Coredump manager
#
###############################################################################
[Unit]
Description=Coredump manager service
Before=dlt-system.service
Wants=dlt-system.service

[Service]
Type=simple
ExecStart=@CMAKE_INSTALL_PREFIX@/bin/coremanager
WatchdogSec=30

[Install]
WantedBy=multi-user.target
